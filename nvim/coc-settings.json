{
    "clangd.fallbackFlags": ["-std=c++17"],
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/14.0.3/clangd_14.0.3/bin/clangd",
    "coc.source.ultisnips.filetypes": ["tex"],
    "diagnostic.errorSign": "EE",
    "diagnostic.warningSign": "WW",
    "diagnostic.infoSign": "II",
    "diagnostic.hintSign": "HH",
    "suggest.noselect": false,
    "suggest.echodocSupport": true,
    "codeLens.enable": true,
    "signature.enable": true,
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "eslint.autoFixOnSave": true,
    "tsserver.implicitProjectConfig.experimentalDecorators": true,
    "python.venvFolders": ["~/.pyenv", "~/.local/share/virtualenvs", "~/.cache/pypoetry/virtualenvs", "~/miniforge3/envs"],
    "python.workspaceSymbols.exclusionPatterns": [],
    "python.jediEnabled": false,
    "python.linting.pylintEnabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pylintUseMinimalCheckers": false,
    "python.linting.pylintPath": "pylint",
    "python.linting.pylintArgs": ["--max-line-length", "120"]
}
